package framse;

import base.comp.BaseFrame;
import base.comp.BaseLable;
import base.comp.ImageLable;

public class MainFrame extends BaseFrame{

	public MainFrame() {
		// TODO Auto-generated constructor stub
		super.BaseFrame("메인", 700, 600, null);
		
	}

	@Override
	public void setComp() {
		// TODO Auto-generated method stub
		
		
	}

	@Override
	public void setDesign() {
		// TODO Auto-generated method stub
		jpTop.addChild();
		jpTop.jpCenter.add(new ImageLable(null, "구름.jpg", 545, 200));
		jpTop.jpBottom.add(new BaseLable("SKY AIRLINE", 30).setCenter());
		jpCenter.add(new ImageLable(null, "비행기.jpg", 150, 150).setCenter());
		
	}

	@Override
	public void events() {
		// TODO Auto-generated method stub
		
	}
	
	
}
